{
  "name": "forumgs",
  "version": "1.0.0",
  "description": "This project will be a web application that will work as a virtual forum",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "migrations:generate": "sequelize-cli migration:generate  --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [
    "javascript",
    "expressjs",
    "forum"
  ],
  "author": "Gabriel Fajardo <gabo2023brazil@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "mysql2": "^2.3.3",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.13.0"
    "express": "^4.17.2",
    "faker": "^5.5.3",
    "joi": "^17.5.0"
  },
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "sequelize-cli": "^6.3.0"
  }  
}
